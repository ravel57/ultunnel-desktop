{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "singbox",
  "description": "Capability for Sing-box integration",
  "windows": ["main"],
  "permissions": [
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "bin/sing-box",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}